@using AllPaczkino.Repositories;
@model IEnumerable<AllPaczkino.Models.ParcelLocker>

@{
    ViewData["Title"] = "Index";
}



<p>
    <a asp-action="Create">Create New</a>
    <button class="create-button" margin-top: 100px;></button>
</p>

<table class="table table-striped table-dark"style=" margin-top: 125px;">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">@Html.DisplayNameFor(model => model.id)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.city)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.postal_code)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.address)</th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">
                    @Html.DisplayFor(modelItem => item.id)
                </th>
                <td>
                    @Html.DisplayFor(modelItem => item.id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.city)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.postal_code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.address)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.id })
                </td>
            </tr>
        }
    </tbody>
</table>
